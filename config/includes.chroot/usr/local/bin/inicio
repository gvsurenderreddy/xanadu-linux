#!/bin/bash
# Elaborado por Sinfallas
archiver="/home/user/.config/xarchiver/xarchiverrc"
sudo rmmod -f ip_tables ip6_tables iptable_filter iptable_mangle iptable_nat iptable_raw bluetooth rfcomm rfkill
if [[ -f $archiver ]]; then
	source /usr/local/bin/post
else
	mkdir -p /home/user/.config/xarchiver/
	mv -f /home/user/xarchiverrc /home/user/.config/xarchiver/xarchiverrc
fi
exit 0
